      if (key.toLowerCase() === 'it') {
        var params = {};
        params[key] = urlParams[key];
        jobUrl = insertParametertoUrl(jobUrl, params);
      }
    }
  }
 
  var jobRefferer = getUrlParameter(null, 'source');
 
  var jobUrlFromJobOject = _.get(result, 'apply_url', jobUrl);

